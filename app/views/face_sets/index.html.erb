<p id="notice"><%= notice %></p>

<h1>Listing Face Sets</h1>

<table>
  <thead>
    <tr>
      <th>Token</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @face_sets.each do |face_set| %>
      <tr>
        <td><%= face_set.token %></td>
        <td><%= link_to 'Show', face_set %></td>
        <td><%= link_to 'Edit', edit_face_set_path(face_set) %></td>
        <td><%= link_to 'Destroy', face_set, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Face set', new_face_set_path %>
